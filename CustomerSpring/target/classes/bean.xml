<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="com.Customer.entity.Customer" id="customerBeans" scope="prototype" >
         <property name="code" value="1234"></property>
		<property name="name" value="agnee"></property>
		<property name="address" value="Grantstreet"></property>
		<property name="product" ref="productBeans"></property>
		</bean>

	
	<bean class="com.Customer.entity.Product" id="productBeans">
         <property name="pcode" value="134"></property>
		<property name="pname" value="tea"></property>
		<property name="stock" value="121"></property>
		<property name="price" value="1666.00"></property>
		
		
		
		
		
	</bean>
	</beans>
		